    "@types/bcryptjs": "^2.4.3",
    "@types/chai": "^4.3.6",
    "@types/diff": "^5.0.3",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/normalize-path": "^3.0.0",
    "@types/pako": "^2.0.1",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vscode/l10n-dev": "^0.0.29",
    "@vscode/test-electron": "^2.3.2",
    "chai": "^4.3.8",
    "copy-webpack-plugin": "^11.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "cyia-code-util": "^2.2.1",
    "esbuild-loader": "^4.0.2",
    "eslint": "^8.46.0",
    "glob": "^10.3.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.5.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@angular-devkit/core": "^16.2.8",
    "@angular-devkit/schematics": "^16.2.8",
    "@angular/compiler": "^16.2.11",
    "@lezer/javascript": "^1.4.8",
    "@lezer/python": "^1.1.9",
    "@lezer/xml": "^1.0.3",
    "@trpc/client": "10.38.5",
    "@vscode/vsce": "^2.21.0",
    "@vue/compiler-dom": "^3.3.7",
    "bcryptjs": "^2.4.3",
    "diff": "^5.1.0",
    "fast-equals": "^5.0.1",
    "fast-glob": "^3.3.1",
    "fastest-levenshtein": "^1.0.16",
    "file-type": "^18.5.0",
    "handlebars": "^4.7.8",
    "ignore": "^5.2.4",
    "interval-operations": "^2.0.0",
    "lodash": "^4.17.21",
    "minimatch": "^9.0.3",
    "node-fetch": "^3.3.2",
    "normalize-path": "^3.0.0",
    "pinyin-pro": "^3.17.0",
    "rfdc": "^1.3.0",
    "rxjs": "^7.8.1",
    "slash": "^5.1.0",
    "snappyjs": "^0.7.0",
    "static-injector": "^2.2.1",
    "ts-node": "^10.9.1",
    "ts-to-zod": "^3.1.3",
    "typescript": "5.2.2",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.3"
  }
}
